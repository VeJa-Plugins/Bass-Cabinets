@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://VeJaPlugins.com/plugins/Release/FreqGen>
a lv2:Plugin, mod:ControlVoltagePlugin;
lv2:requiredFeature urid:map ;

doap:name "Freq Generator";

doap:developer [
    foaf:name "VeJa plugins";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "JeJa plugins";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
can we generate some banging sine waves?
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort, mod:CVPort;
    lv2:index 0;
    lv2:symbol "CvInput";
    lv2:name "CV Input";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "AudioOut";
    lv2:name "Output";
].